class Solution{
public int[] twosum(int[] nums,int Target){
int n = nums.length;
    for(int i=0;i<=n-1;i++){
        for(int j = i+1 ; j<=n;j++){
              if(nums[i]+nums[j] == Target){
                     return new int[]{i,j};
              }
        }
    }
        return new int[]{};
  }
}
